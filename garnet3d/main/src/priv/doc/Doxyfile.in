# Doxyfile 1.4.6
PROJECT_NAME           = Garnet3D
OUTPUT_DIRECTORY       = "$OUTPUT_DIRECTORY"
#OUTPUT_LANGUAGE        = Chinese
TAB_SIZE               = 4
EXTRACT_LOCAL_CLASSES  = NO
JAVADOC_AUTOBRIEF      = YES
HIDE_IN_BODY_DOCS      = NO
SORT_BY_SCOPE_NAME     = YES
QUIET                  = YES
WARNINGS               = NO
WARN_IF_UNDOCUMENTED   = NO
INPUT                  = src/priv
FILE_PATTERNS          = *.h *.inl
EXCLUDE                = src/priv/experimental src/priv/gfx/rndr
EXCLUDE_PATTERNS       = pch.h pch.cpp
RECURSIVE              = YES
GENERATE_HTMLHELP      = "$GENERATE_HTMLHELP"
CHM_FILE               = "$CHM_FILE"
HHC_LOCATION           = "$HHC_LOCATION"
GENERATE_TREEVIEW      = YES
GENERATE_LATEX         = NO
MACRO_EXPANSION        = YES
PREDEFINED             = GN_MSWIN \
                         GN_POSIX \
                         GN_CASSERT(X)="" \
                         THIS THIS_ \
                         STDMETHODCALLTYPE \
                         STDMETHOD(X)="virtual HRESULT X" \
                         STDMETHOD_(R,X)="virtual R X"